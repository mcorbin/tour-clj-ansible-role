[Unit]
Description=Tour of Clojure, your favorite Clojure tutorial
After=network.target

[Service]
User={{ tour_user }}
Environment=CONFIG_PATH={{ tour_config_path }}
Group={{ tour_user }}
ExecStart=/usr/bin/java -Xms{{ tour_java_memory }} -Xmx{{ tour_java_memory }} XX:+ExitOnOutOfMemoryError -jar {{ tour_jar_path }}
Restart=on-failure

[Install]
WantedBy=multi-user.target